{
  "name": "aums_desktop",
  "version": "1.0.0",
  "description": "AUMS Desktop Client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "hu.aums.desktop",
    "productName": "AUMS",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "nsis": {
    "artifactName": "AUMS-Desktop.exe"
  },
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^15.3.2",
    "electron-builder": "^22.13.1"
  }
}
